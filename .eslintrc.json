{
  "extends": ["airbnb", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/no-cycle": [
      "error",
      {
        "maxDepth": 1
      }
    ],
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ],
    "no-unused-vars":[0],
    "prefer-const":[0],
    "react/self-closing-comp":[0]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
